@model List<ResultHomeDto>
@{
    ViewData["Title"] = "AHome";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Baslik</th>
        <th>Description</th>
        <th>ImageUrl</th>
        <th>Sil</th>
        <th>Güncelle</th>
        <th>Detaylar</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <th>@item.Id</th>
            <th>@item.Baslik</th>
            <td>@item.Description</td>
            <td>@item.ImageUrl</td>
            <td><a href="/AdminHome/DeleteHome/@item.Id" class="btn mb-1 btn-rounded btn-outline-danger">Sil</a></td>
            <td><a href="/AdminHome/UpdateHome/@item.Id" class="btn mb-1 btn-rounded btn-outline-success">Güncelle</a></td>
            <td><a href="#" class="btn mb-1 btn-rounded btn-outline-dark">Detaylar</a></td>
        </tr>
    }
</table>
<a href="/AdminHome/AddHome/" class="btn mb-1 btn-rounded btn-outline-info">Home</a>


